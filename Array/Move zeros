import java.io.*;
import java.util.*;
public class Solution {
class Solution {
    public void moveZeroes(int[] arr) {
        int  n = arr.length;
        int count =0;
        for(int i=0;i<n;i++){ 
              if (arr[i]!=0){ 
                 arr[count] = arr[i];
                   count++;
               }                
        } while(count<arr.length){
            arr[count++]=0;
     } System.out.println(Arrays.toString(arr));
    }
}
}
